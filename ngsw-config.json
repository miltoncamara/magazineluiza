{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/*.scss"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name":"api",
      "urls":["/atlas"],
      "cacheConfig":{
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "3d"
      }
    },
    {
      "name":"api",
      "urls":["/destaques"],
      "cacheConfig":{
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "3d"
      }
    }, 
    {
      "name":"api",
      "urls":["/comercialPlain"],
      "cacheConfig":{
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "3d"
      }
    },
    {
      "name":"api",
      "urls":["/specialProjects"],
      "cacheConfig":{
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "3d"
      }
    }   
  ]
}
